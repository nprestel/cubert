<fieldset>
  <%= f.label :piece_name, "Name", class: "control-label" %>
  <%= f.text_field :piece_name, class: "form-control" %><br>
  <div class="form-group col-xs-2">
    <%= f.label :length_ins, "Length", class: "control-label" %>
    <%= f.number_field :length_ins, class: "form-control"  %>
    <%= f.label :width_ins, "Width", class: "control-label" %>
    <%= f.number_field :width_ins, class: "form-control"  %>
    <%= f.label :height_ins, "Height", class: "control-label" %>
    <%= f.number_field :height_ins, class: "form-control"  %><br>
  </div>
  <%= f.label :weight_lbs, "Weight" %>
  <%= f.number_field :weight_lbs, class: "form-control"  %><br>
  <%= f.label :stackability, "Stackability" %>
  <%= f.number_field :stackability, class: "form-control"  %><br>
  <%= f.label :count, "How Many" %>
  <%= f.number_field :count, class: "form-control"  %><br>
  <%= f.check_box :_destroy %>
  <%= f.label :_destroy, "Remove Piece" %>
</fieldset>